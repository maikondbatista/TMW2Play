{
  "name": "frontend",
  "version": "1.0.8",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "bump": "npm version patch --no-git-tag-version",
    "build:github": "npm run bump && ng build --configuration github && npm run postbuild:github",
    "postbuild:github": "powershell -Command \"if (Test-Path '../docs/browser') { Get-ChildItem '../docs/browser' | Move-Item -Destination '../docs' -Force; Remove-Item '../docs/browser' -Force }\"",
    "build:docker": "ng build --configuration docker"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^21.0.6",
    "@angular/common": "^21.1.5",
    "@angular/compiler": "^21.0.6",
    "@angular/core": "^21.0.6",
    "@angular/forms": "^21.0.6",
    "@angular/platform-browser": "^21.0.6",
    "@angular/platform-browser-dynamic": "^21.0.6",
    "@angular/router": "^21.0.6",
    "@jsverse/transloco": "^8.2.0",
    "@ng-bootstrap/ng-bootstrap": "^20.0.0",
    "@popperjs/core": "^2.11.8",
    "bootstrap": "^5.3.8",
    "chart.js": "^4.5.1",
    "ng2-charts": "^8.0.0",
    "ngx-cookie-service": "^21.1.0",
    "ngx-toastr": "^19.1.0",
    "rxjs": "^7.8.2",
    "tslib": "^2.8.1",
    "zone.js": "^0.16.0"
  },
  "devDependencies": {
    "@angular/build": "^21.0.4",
    "@angular/cli": "^21.0.4",
    "@angular/compiler-cli": "^21.0.6",
    "@angular/localize": "^21.0.6",
    "@types/jasmine": "^5.1.13",
    "jasmine-core": "~5.6.0",
    "karma": "^6.4.4",
    "karma-chrome-launcher": "^3.2.0",
    "karma-coverage": "^2.2.1",
    "karma-jasmine": "^5.1.0",
    "karma-jasmine-html-reporter": "^2.1.0",
    "typescript": "^5.9.3"
  }
}
